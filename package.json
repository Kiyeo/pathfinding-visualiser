    "@types/node": "17.0.24",
    "@types/react": "18.0.5",
    "@types/react-dom": "^18.0.1",
    "@types/styled-components": "^5.1.18",
    "babel-plugin-styled-components": "^2.0.2",
    "gh-pages": "^3.1.0",
    "react-app-rewired": "^2.2.1",
    "typescript": "4.6.3",
